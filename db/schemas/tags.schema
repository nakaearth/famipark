create_table "tags", force: true, options: "ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_bin ROW_FORMAT=DYNAMIC" do |t|
  t.text     "name",        limit: 30,                null: false
  t.integer  "place_id",    limit: 4,     default: 0, null: false
  t.datetime "created_at"
  t.datetime "updated_at"
end

add_index "tags", ["place_id"], name: "index_tags_on_place_id", using: :btree
